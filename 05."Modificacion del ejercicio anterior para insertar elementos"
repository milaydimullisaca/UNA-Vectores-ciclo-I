#include <iostream>
using namespace std;

int main() {
    int num = 5;  // Tamaño del arreglo
    float arreglo[num]; // Declarar un arreglo de tamaño 5
// Solicitar al usuario que introduzca 5 números
    cout << "Introduzca los 5 numeros:" << endl;
    for (int i = 0; i < num; i++) {
        cout << "Número " << i + 1 << ": ";
        cin >> arreglo[i];  // Guardar el número en el arreglo
    }

    // Mostrar los valores del arreglo
    cout << "\nLos valores introducidos son:" << endl;
    for (int i = 0; i < num; i++) {
        cout << "Valor " << i + 1 << ": " << arreglo[i] << endl;
    }

    return 0;  // Finalizar el programa
}
